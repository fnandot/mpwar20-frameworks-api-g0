{
    "type": "project",
    "license": "proprietary",
    "name": "letnando/lasalle-group-zero",
    "description": "Just a project to learn Symfony",
    "require": {
        "php": "8.0.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ramsey/uuid": "^3.8",
        "ruflin/elastica": "^7.1.1",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.14",
        "friendsofphp/php-cs-fixer": "^2.16",
        "friendsoftwig/twigcs": "^5.0.0",
        "fzaninotto/faker": "^1.9",
        "phpmd/phpmd": "^2.7",
        "phpro/grumphp": "^0.17.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LaSalle\\GroupZero\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaSalleTests\\GroupZero\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "symfony security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}

{
    "type": "project",
    "license": "proprietary",
    "name": "fnandot/lasalle-group-zero",
    "description": "Just a project to learn Symfony",
    "require": {
        "php": "^7.3|^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/annotations": "^1.12",
        "friendsofsymfony/rest-bundle": "^3.0",
        "lcobucci/jwt": "^4.1.4",
        "ramsey/uuid": "^3.8",
        "ruflin/elastica": "^7.1.1",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/filesystem": "5.2.*",
        "symfony/finder": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/google-mailer": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/mercure": "^0.4.1",
        "symfony/mercure-bundle": "^0.2.3",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/templating": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.2.*",
        "symfony/var-dumper": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.2.*",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/string-extra": "^3.0",
        "yzalis/identicon": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "friendsoftwig/twigcs": "^5.0.0",
        "phpmd/phpmd": "^2.7",
        "phpro/grumphp": "^1.3.1",
        "rector/rector": "^0.10.3",
        "sensiolabs/security-checker": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LaSalle\\GroupZero\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaSalleTests\\GroupZero\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
